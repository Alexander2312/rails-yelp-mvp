<!-- new.html.erb -->
<ul>
<%= form_for (@restaurant) do |f|%>
  <li>
  <%= f.text_field :name %>
  <%= f.label :name %>

  <%= f.text_field :address %>
  <%= f.label :address %>

  <%= f.text_field :phone_number %>
  <%= f.label :phone_number %>

  <%= f.text_field :category %>
  <%= f.label :category %>

  <%= f.submit 'Create a restaurant' %>
  <% end %>



  </li>
</ul>
